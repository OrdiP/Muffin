<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:m="urn:import:gwt.material.design.client.ui">
  <m:MaterialPanel>
    <div class="input-field" >
      <m:MaterialRow>
        <m:MaterialColumn grid="s6">
          <i class="mdi-action-search prefix"></i>
          <input type="text" ui:field="searchBox" placeholder="Type to search..."/>
        </m:MaterialColumn>
        <m:MaterialColumn grid="s6">
          <m:MaterialButton ui:field="searchBtn" text="Search" waves="light" size="large"/>
        </m:MaterialColumn>
      </m:MaterialRow>
      <m:MaterialContainer ui:field="cardsPanel"></m:MaterialContainer>
    </div>
  </m:MaterialPanel>


</ui:UiBinder>