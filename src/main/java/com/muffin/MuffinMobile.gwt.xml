<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module>

  <inherits name="com.mvu.theme.ionic.Ionic"/>
  <inherits name="gwt.material.design.GwtMaterialDesign"/>
  <inherits name="com.mvu.theme.bootstrap.Bootstrap"/>

  <entry-point class='com.muffin.client.Muffin'/>

  <replace-with class="com.muffin.client.MuffinMobileConfiguration">
    <when-type-is class="com.mvu.core.client.CoreConfiguration"/>
  </replace-with>

  <source path='client' />
  <source path='shared' />
</module>
